{
  "extends": [
    "tslint:latest",
    "tslint-react"
  ],
  "rules": {
    "ter-indent": [
      2,
      {
        "SwitchCase": 1
      }
    ],
    "jsdoc-format": false,
    "no-trailing-whitespace": false,
    "semicolon": [
      true,
      "always"
    ],
    "no-submodule-imports": [
      false
    ],
    "interface-name": [
      false
    ],
    "prefer-object-spread": false,
    "object-literal-sort-keys": false,
    "quotemark": [
      true,
      "single"
    ],
    "ordered-imports": false,
    "jsx-no-lambda": false,
    "member-ordering": false,
    "no-implicit-dependencies": false,
    "no-var-requires": false
  }
}
